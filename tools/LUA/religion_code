local truefounder = true
local foundreligion = false;

if foundreligion then
for iPlayer = 0, 62 do
	local pPlayer = Players[iPlayer]
	
	
		if pPlayer:isAlive() then
		local sCivilizationType = GameInfo.Civilizations[pPlayer:GetCivilizationType()].Type

			for i,Urelig in pairs(treligion) do
				if Urelig.pl==sCivilizationType and not truefounder and not Urelig.rl=="NO_RELIGION" and not Urelig.rl=="RELIGION_PANTHEON" then
					for pCity in pPlayer:Cities() do
						if pCity.GetName == Urelig.cid then 
							Game.FoundReligion(iPlayer, Urelig.rl,nil, Urelig.b1, Urelig.b2, Urelig.b3, -1, -1, pCity)
							Game.EnhanceReligion(iPlayer, Urelig.rl, Urelig.b4, Urelig.b5)
							Game.EnhanceReligion(iPlayer, Urelig.rl, Urelig.b6, nil)
							
								--break
						
						end
					
					end
					
				end
				if Urelig.fo==sCivilizationType and truefounder and not Urelig.rl=="NO_RELIGION" and not Urelig.rl=="RELIGION_PANTHEON" then
					for pCity in pPlayer:Cities() do
						if pCity.GetName == Urelig.cid then 
							Game.FoundReligion(iPlayer, Urelig.rl,nil, Urelig.b1, Urelig.b2, Urelig.b3, -1, -1, pCity)
							Game.EnhanceReligion(iPlayer, Urelig.rl, Urelig.b4, Urelig.b5)
							Game.EnhanceReligion(iPlayer, Urelig.rl, Urelig.b6, nil)

								--break
						
						end
					
					end
					
				end
				

			end
			

		end
	


	
	end
	
for iPlayer = 0, 62 do
	local pPlayer = Players[iPlayer]
	
	
		if pPlayer:isAlive() then
		local sCivilizationType = GameInfo.Civilizations[pPlayer:GetCivilizationType()].Type
				

				for i,Rst in pairs(stcities) do
					if Rst.owner==sCivilizationType then
						for pCity in pPlayer:Cities() do
						if pCity.GetName==Rst.ctname then
							pCity:AdoptReligionFully(GameInfoTypes[Rst.relgn]);
						end
					end
				end
			
			end
		end
		
	end
	
	end
--end
	
--end
	





